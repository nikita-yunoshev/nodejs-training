{
  "name": "js-training",
  "version": "1.0.0",
  "description": "Node.js + Postgres",
  "author": "Nikita Yunoshev",
  "main": "server.js",
  "scripts": {
    "start": "node ./bin/www",
    "devstart": "nodemon ./bin/www",
    "migrate-latest": "knex migrate:latest --knexfile knexfile.js",
