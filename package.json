    "migrate-make": "knex migrate:make",
    "seed-make": "knex seed:make",
    "lint": "eslint .",
    "test": "node ./node_modules/mocha/bin/mocha"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
