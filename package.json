    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "knex": "^0.20.1",
    "morgan": "~1.9.1",
    "pg": "^7.12.1"
  },
  "devDependencies": {
    "eslint": "^6.6.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "mocha": "1.20.1",
    "nodemon": "^1.14.12",
    "supertest": "0.13.0"
  },
  "pre-commit": [
    "lint"
  ]
